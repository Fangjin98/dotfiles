alias ls='lsd'
alias la='ls -A'
alias ll='ls -l'
alias lla='ls -la'

alias gpo='git push origin master'
alias gp='git pull'
alias gs='git status'
alias gd='git diff'
alias gr='git reflog'
alias glf='git ls-files'
alias pgp='proxyon && git pull'
alias pgpo='proxyon && git push origin master'

alias gogo='source ~/.zshrc'
